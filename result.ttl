@prefix : <http://example.org/ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Define the speaker ("I") as a Person
:speaker
    a schema:Person .

# Define Kyle as a Person
:Kyle
    a schema:Person ;
    schema:name "Kyle" .

# Define the Lost Bean cafe
:lostBeanCafe
    a schema:CafeOrCoffeeShop ;
    schema:name "Lost Bean Cafe" .

# Describe the meeting event
:meetingWithKyle
    a schema:MeetAction ;
    schema:agent :speaker ; # The person performing the action (the "I")
    schema:participant :Kyle ; # The other participant in the meeting
    schema:location :lostBeanCafe ;
    schema:startTime "yesterday in the morning"^^xsd:string . # The time is relative, so xsd:string is used